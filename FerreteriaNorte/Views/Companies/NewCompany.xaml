<Page x:Class="FerreteriaNorte.Views.Companies.NewCompany"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FerreteriaNorte.Views.Companies"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NewCompany">

    <Grid>
        <GroupBox x:Name="groupBox" Header="Nueva Empresa" Margin="0,0,0,0" VerticalAlignment="Stretch">
            <Grid HorizontalAlignment="Left" Height="414" Margin="0,10,-2,-4" VerticalAlignment="Top" Width="790">
                <Label x:Name="label" Content="Razón Social" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontWeight="Normal" FontSize="12"/>
                <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="92,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="300"/>
                <Label x:Name="label_Copy" Content="Razón Social" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" FontWeight="Normal" FontSize="12"/>
                <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="92,38,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="300"/>
            </Grid>
        </GroupBox>
    </Grid>
</Page>
